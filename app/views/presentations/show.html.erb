<% if @presenter %>
	<input type="hidden" id="admin" />
<% end %>

<% if @error %>
	<p><%= @error %></p>
<% else %>
<div id="container">
	<img src="assets/empty.png" id="image" data-poll-url="<%= presentation_current_slide_path(@presentation) %>" style="display: none; position: absolute;"/>
	<img src="assets/empty.png" id="image_swap" style="display: none; position: absolute;" >
</div>
<% end %>